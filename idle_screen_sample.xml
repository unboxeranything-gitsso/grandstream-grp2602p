<?xml version="1.0" encoding="UTF-8"?>
<!-- This XML template example adds a custom logo in the center of the idle screen display -->
<!-- Date is displayed on top left and time on the top right -->
<Screens>
  <Screen model="GRP2603;GRP2604" width="132" height="64">
    <!-- Main screen -->
    <IdleScreen>
      <ShowStatusLine>true</ShowStatusLine>
      <DisplayElement>
        <!-- Display time  -->
        <DisplayString halign="right" width="117" height="14" font="time_14">
          <DisplayStr>$T</DisplayStr>
          <X>0</X>
          <Y>27</Y>
        </DisplayString>
        <!-- Display AM/PM  -->
        <DisplayString halign="right" width="132" height="14" font="time">
          <DisplayStr>$P</DisplayStr>
          <X>0</X>
          <Y>30</Y>
        </DisplayString>
        <!-- Display Month-week-date -->
        <DisplayString halign="right" width="132" height="10">
          <DisplayStr>$f</DisplayStr>
          <X>0</X>
          <Y>40</Y>
        </DisplayString>
      </DisplayElement>
      <!-- Display Logo or Vendor Name (middle center) -->
      <DisplayBitmap isfile="false" isflash="false">
        <Bitmap></Bitmap>
        <X>80</X>
        <Y>-5</Y>
      </DisplayBitmap>
      <!-- Define available softkeys here -->
      <SoftKeys>
        <SoftKey>
          <Action>
            <SwitchSCR />
          </Action>
          <displayCondition>
            <conditionType>SubScreen</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <BSCallCenter />
          </Action>
          <displayCondition>
            <conditionType>bsCallCenter</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <MissedCalls />
          </Action>
          <displayCondition>
            <conditionType>missCall</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <FwdAll />
          </Action>
          <displayCondition>
            <conditionType>callFwdCancelled</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <CancelFwd />
          </Action>
          <displayCondition>
            <conditionType>callFwded</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <CallParked />
          </Action>
          <displayCondition>
            <conditionType>hasBWCallParks</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <LDAP />
          </Action>
          <displayCondition>
            <conditionType>LDAPConfigured</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <Redial />
          </Action>
          <displayCondition>
            <conditionType>hasDialedCalllog</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <VoiceMail />
          </Action>
          <displayCondition>
            <conditionType>hasVoiceMail</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <PhoneBook />
          </Action>
        </SoftKey>
        <SoftKey>
          <Action>
            <Login />
          </Action>
          <displayCondition>
            <conditionType>logout</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <Logout />
          </Action>
          <displayCondition>
            <conditionType>login</conditionType>
          </displayCondition>
        </SoftKey>
        <SoftKey>
          <Action>
            <Dial>
              <Account>0</Account>
              <Number>1000</Number>
            </Dial>
          </Action>
          <Label>1000</Label>
        </SoftKey>
      </SoftKeys>
      <!-- End of softkey definition -->
    </IdleScreen>
    <IdleScreen>
      <ShowStatusLine>false</ShowStatusLine>
      <DisplayElement>
        <!-- Display time  -->
        <DisplayString halign="right" width="105" height="14" font="time_14">
          <DisplayStr>$T</DisplayStr>
          <X>0</X>
          <Y>17</Y>
        </DisplayString>
        <!-- Display AM/PM  -->
        <DisplayString halign="right" width="120" height="14" font="time">
          <DisplayStr>$P</DisplayStr>
          <X>0</X>
          <Y>20</Y>
        </DisplayString>
        <!-- Display Month-week-date -->
        <DisplayString halign="right" width="120" height="10">
          <DisplayStr>$f</DisplayStr>
          <X>0</X>
          <Y>30</Y>
        </DisplayString>
      </DisplayElement>
      <!-- Display Logo or Vendor Name (middle center) -->
      <DisplayBitmap isfile="false" isflash="false">
        <Bitmap></Bitmap>
        <X>25</X>
        <Y>15</Y>
      </DisplayBitmap>
      <!-- Define available softkeys here -->
      <SoftKeys>
        <SoftKey>
          <Action>
            <SwitchSCR />
          </Action>
          <displayCondition>
            <conditionType>SubScreen</conditionType>
          </displayCondition>
        </SoftKey>
      </SoftKeys>
      <!-- End of softkey definition -->
    </IdleScreen>
    <!-- Sub-screen that show IP and Account info -->
    <IdleScreen>
      <ShowStatusLine>false</ShowStatusLine>
      <DisplayString>
        <DisplayStr>$+161: $I</DisplayStr>
        <X>2</X>
        <Y>2</Y>
      </DisplayString>
      <DisplayString>
        <DisplayStr>$+339: $-O</DisplayStr>
        <X>2</X>
        <Y>16</Y>
      </DisplayString>
      <SoftKeys>
        <SoftKey>
          <Action>
            <SwitchSCR />
          </Action>
          <displayCondition>
            <conditionType>SubScreen</conditionType>
          </displayCondition>
        </SoftKey>
      </SoftKeys>
    </IdleScreen>
  </Screen>

